@model PublishArticleModel
@{
    ViewData["Title"] = "Publish Title";
}

<form method="post">
    <div class="form-group">
        <label asp-for="Title"></label>
        <input asp-for="Title" type="text" class="form-control" />
        <span asp-validation-for="Title" class="form-text text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Content"></label>
        <input asp-for="Content" type="text" class="form-control" />
        <span asp-validation-for="Content" class="form-text text-danger"></span>
    </div>
    <div>
        <button type="submit" class="btn btn-success" value="Publish Article">Publish</button>
    </div>
</form>

